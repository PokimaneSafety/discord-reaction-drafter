{
    "name": "@pokimane-safety/discord-reaction-drafter",
    "version": "1.0.1",
    "description": "Randomly drafts Discord message reactors.",
    "type": "commonjs",
    "main": "src/index.js",
    "types": "build/src/index.d.ts",
    "engines": {
        "node": ">= 16.6.0"
    },
    "license": "GPL-3.0-or-later",
    "keywords": [],
    "scripts": {
        "start": "./scripts/start.js",
        "start:watch": "./scripts/start.js -w",
        "build": "./scripts/build.sh",
        "typecheck": "tsc -p tsconfig.json --noEmit",
        "clean": "rimraf bin build coverage yarn-error.log .eslintcache",
        "lint": "eslint './**/*.{js,ts}'",
        "lint:fix": "yarn lint --fix",
        "format": "prettier --list-different './**/*.{js,ts,json,md,yaml}'",
        "format:fix": "prettier --write './**/*.{js,ts,json,md,yaml}'",
        "spelling": "cspell --config=.cspell.json './**/*.{js,ts,md}'",
        "test": "jest -c .jestrc.js --verbose --passWithNoTests",
        "test:watch": "yarn test --watchAll --verbose=false",
        "test:coverage": "yarn test --coverage",
        "test:coverage:watch": "yarn test:coverage  --watchAll --verbose=false",
        "prepare": "husky install || exit 0"
    },
    "dependencies": {
        "cli-progress": "^3.9.1",
        "discord.js": "^12",
        "inquirer": "^8.2.0"
    },
    "devDependencies": {
        "@types/cli-progress": "^3.9.2",
        "@types/inquirer": "^8.1.3",
        "@types/jest": "^26.0.13",
        "@types/node": "^18.0.3",
        "@typescript-eslint/eslint-plugin": "^4.28.5",
        "@typescript-eslint/parser": "^4.0.1",
        "commander": "^8.3.0",
        "cspell": "^5.3.7",
        "eslint": "^7.32.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-eslint-comments": "^3.2.0",
        "eslint-plugin-eslint-plugin": "^3.0.3",
        "eslint-plugin-import": "^2.23.4",
        "eslint-plugin-jest": "^24.4.0",
        "eslint-plugin-optimize-regex": "^1.2.1",
        "eslint-plugin-prettier": "^3.1.4",
        "eslint-plugin-simple-import-sort": "^7.0.0",
        "eslint-plugin-sort-keys-fix": "^1.1.1",
        "husky": "^7.0.4",
        "jest": "^26.4.2",
        "lint-staged": "^11.1.1",
        "nexe": "^4.0.0-beta.19",
        "prettier": "^2.3.2",
        "rimraf": "^3.0.2",
        "ts-jest": "^26",
        "ts-node": "^10.2.1",
        "ts-node-dev": "^1.1.8",
        "typescript": "^4.3.5"
    }
}
